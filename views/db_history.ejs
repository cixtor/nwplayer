
<table class="table table-bordered dbhistory">
    <thead>
        <tr>
            <% fields.forEach(function(field){ %>
                <th><%= field %></th>
            <% }) %>
        </tr>
    </thead>
    <tbody>
        <% if( videos.length > 0 ){ %>
            <% videos.forEach(function(item){ %>
                <tr>
                    <% fields.forEach(function(field){ %>
                        <% if( field == 'video_id' ){ %>
                            <td><a href="<%= item['url'] %>" target="_self"><%= item['title'] %></a></td>
                        <% } else if( field == 'updated_at' ){ %>
                            <td><%= item[field] %></td>
                        <% } else { %>
                            <td><%= item[field] %></td>
                        <% } %>
                    <% }) %>
                </tr>
            <% }) %>
        <% } else { %>
            <tr>
                <td colspan="<%= fields.length %>">
                    <i>The result of this query is empty.</i>
                </td>
            </tr>
        <% } %>
    </tbody>
</table>
